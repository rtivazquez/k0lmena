{
  "name": "@artilleryio/playwright-reporter",
  "version": "1.1.2",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/artilleryio/playwright-reporter.git"
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs --dts --clean",
    "watch": "npm run build -- --watch src",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "node tests/basic.mjs"
  },
  "keywords": [
    "playwright",
    "reporter"
  ],
  "author": "Artillery.io <team@artillery.io>",
  "license": "SEE LICENSE IN LICENSE.txt",
  "description": "Playwright reporter for Artillery Cloud",
  "devDependencies": {
    "@playwright/test": "^1.50.1",
    "@types/debug": "^4.1.12",
    "@types/node": "^22.9.0",
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@actions/core": "^1.11.1",
    "@actions/github": "^6.0.0",
    "ci-info": "^4.1.0",
    "debug": "^4.3.7",
    "got": "^11.8.5",
    "nanoid": "^3.3.4"
  }
}
